<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PrimeroActivityMoscoso">

    <TextView
        android:id="@+id/invertidotextView"
        android:layout_width="144dp"
        android:layout_height="33dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="332dp"
        android:text="TextView"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dividendotextView"
        android:layout_width="144dp"
        android:layout_height="33dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="196dp"
        android:text="TextView"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/divisortextView"
        android:layout_width="143dp"
        android:layout_height="36dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="248dp"
        android:text="TextView"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nombretextView"
        android:layout_width="144dp"
        android:layout_height="33dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="40dp"
        android:text="TextView"
        android:textIsSelectable="false"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/apellidotextView"
        android:layout_width="143dp"
        android:layout_height="36dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="92dp"
        android:text="TextView"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_siguiente"
        android:layout_width="192dp"
        android:layout_height="42dp"
        android:layout_marginStart="108dp"
        android:layout_marginBottom="140dp"
        android:text="Siguiente"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_mostrarResultados"
        android:layout_width="191dp"
        android:layout_height="45dp"
        android:layout_marginStart="108dp"
        android:layout_marginBottom="68dp"
        android:text="Mostrar Resultados"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>